{
	"name": "devdocs-io",
	"displayName": "devdocs-io",
	"description": "Search docs on devdocs.io",
	"version": "0.0.1",
	"publisher": "ReanGD",
	"icon": "images/icon.png",
	"license": "Apache 2.0",
	"homepage": "https://github.com/ReanGD/vscode-devdocs-io/blob/master/README.md",
	"repository": {
        "type": "git",
        "url": "https://github.com/ReanGD/vscode-devdocs-io"
	},
	"bugs": {
        "url": "https://github.com/ReanGD/vscode-devdocs-io/issues"
    },
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
        "devdocs",
        "doc",
        "help",
        "documentation"
    ],
	"activationEvents": [
		"onCommand:devdocs-io.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "devdocs-io.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"npm_install": "npm install",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"eslint": "^6.8.0",
		"typescript": "^3.8.3"
	}
}
